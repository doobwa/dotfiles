xft.dpi:    96
xft.hintstyle: hintslight
xft.autohinting: 1
xft.antialias: 1
xft.rgba: rgb
Xcursor*theme:    Vanilla-DMZ-AA
Xcursor.size: 22

! urxvt.font: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
urxvt.font: xft:inconsolata:size=8:antialias=true:hinting=true
! urxvt.font: xft:deja vu sans mono:size=8:antialias=true:hinting=true
! urxvt.font: -*-montecarlo-medium-*-*-*-*-*-*-*-*-*-*-*
! urxvt.boldFont: -*-montecarlo-bold-*-*-*-*-*-*-*-*-*-*-*

urxvt*geometry:         80x25
urxvt*saveLines:        8192 
urxvt*scrollBar:        false
urxvt*scrollstyle:      plain ! options: rxvt, plain, next or xterm. plain
urxvt*scrollBar_right:  true
urxvt*background:       #222222
urxvt*borderColor:      #222222
urxvt*loginShell:       true
! urxvt.borderLess:      true
urxvt*transparent:	    false 
urxvt*cutchars:         "()*,<>[]{}|'
urxvt*print-pipe:       cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*secondaryScroll:  true
urxvt*mapAlert:         true
urxvt*utmpInhibit:      true
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      firefox
urxvt*matcher.button:   1
urxvt*matcher.pattern.1:\\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
urxvt.urgentOnBell:     true
!urxvt*visualBell:        true

