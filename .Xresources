xft.dpi:    96
Xcursor*theme:    Vanilla-DMZ-AA
Xcursor.size: 22

! urxvt.font: -*-terminus-*-*-*-*-12-*-*-*-*-*-*-*
! urxvt.font: xft:liberation mono:size=10:antialias=true:hinting=false
! urxvt.font: xft:consolas:size=9:antialias=true:hinting=true
urxvt.font: xft:Inconsolata-Regular:size=12:antialias=true:hinting=true
emacs.font: Inconsolata-g-9

urxvt*geometry:         80x25
urxvt*saveLines:        8192 
urxvt*scrollBar:        false
urxvt*scrollstyle:      plain ! options: rxvt, plain, next or xterm. plain
urxvt*scrollBar_right:  true
! urxvt*background:       #222222
! urxvt*foreground:       #EBEBFF
! urxvt*borderColor:      #222222
urxvt*loginShell:       true
urxvt*transparent:	    false 
urxvt*cutchars:         "`\"()*',<>[]{|}"
urxvt*print-pipe:       cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*secondaryScroll:  true
urxvt*mapAlert:         true
urxvt*utmpInhibit:      true
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      firefox
urxvt*matcher.button:   1
urxvt*matcher.pattern.1:\\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
urxvt.urgentOnBell:     true
urxvt*cursorBlink: false 
! Change this hotkey because it conflicts with vim-ipython
URxvt.searchable-scrollback: CM-s

xpdf*enableFreetype:    yes
xpdf*antialias:    yes
xpdf*foreground:    black
xpdf*background:    white
xpdf*urlCommand:    firefox3 %s
xpdf*viKeys:        on
